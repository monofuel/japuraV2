{
 "presets": ["react","es2015","stage-0"],
 "plugins": ["transform-flow-strip-types","syntax-async-functions","transform-regenerator","transform-runtime"],
 "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "livereactload/babel-transform",
            "imports": ["react"]
          }]
        }]
      ]
    }
  }
}
